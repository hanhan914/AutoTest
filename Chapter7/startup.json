[
  {
    "description":"这是我们的第一个mock例子",
    "request":{
      "uri":"/demo",
      "method":"get"
    },
    "response":{
      "text":"这是一个没有参数的get请求",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },

  {
    "description":"这是一个带参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get",
      "queries":{
        "name":"wang",
        "age":"30"
      }
    },
    "response":{
      "text":"wang又回来了！！！",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }
]